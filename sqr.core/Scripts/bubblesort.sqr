funqtion sort (Qollection q { 
	*~ sorted <~ false;
	while (sorted == false) {
		*~ i <~ 0;
		while (i < q:length() - 1) {
			*~ temp <~ q:get(i + 1);
			*~ swapped <~ false;
			if (temp > q:get(i)) {
				q:set(i + 1, q:get(i));
				q:set(i, temp);
				swapped <~ true;
			}
			if (swapped == false) {
				sorted <~ true;
				break;
			}
			i <~ i + 1;
		}
	}
});